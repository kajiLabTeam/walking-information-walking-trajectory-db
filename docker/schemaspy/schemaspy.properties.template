# データベースの種類を指定します。詳細を確認したい場合は -dbhelp を実行してください
schemaspy.t=pgsql

# 任意: 別のJDBCドライバのパスを指定します
schemaspy.dp=/drivers/postgresql.jar

# データベースのプロパティ: ホスト名、ポート番号、データベース名、ユーザー名、パスワード
schemaspy.host=${DB_HOST}
schemaspy.port=${DB_PORT}
schemaspy.db=${DB_NAME}
schemaspy.u=${DB_USER}
schemaspy.p=${DB_PASSWORD}

# 生成されたファイルを保存する出力ディレクトリ
schemaspy.o=./db/details

# ダイアグラムを生成する対象のデータベーススキーマを指定します
schemaspy.schemas=public
